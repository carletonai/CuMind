{
  "CuMind": {
    "representation": {
      "type": "cumind.core.resnet.ResNet",
      "num_blocks": 2,
      "conv_channels": 32
    },
    "dynamics": {
      "type": "cumind.core.mlp.MLPWithEmbedding",
      "num_blocks": 2
    },
    "prediction": {
      "type": "cumind.core.mlp.MLPDual"
    },
    "memory": {
      "type": "cumind.data.memory.MemoryBuffer",
      "capacity": 2000,
      "min_size": 100,
      "min_pct": 0.1,
      "per_alpha": 0.6,
      "per_epsilon": 1e-06,
      "per_beta": 0.4
    },
    "networks": {
      "hidden_dim": 128
    },
    "training": {
      "optimizer": "optax.adamw",
      "batch_size": 64,
      "learning_rate": 0.01,
      "weight_decay": 0.0001,
      "target_update_frequency": 250,
      "checkpoint_interval": 50,
      "num_episodes": 1220,
      "train_frequency": 2,
      "checkpoint_root_dir": "checkpoints"
    },
    "mcts": {
      "num_simulations": 25,
      "c_puct": 1.25,
      "dirichlet_alpha": 0.25,
      "exploration_fraction": 0.25
    },
    "env": {
      "name": "CartPole-v1",
      "action_space_size": 2,
      "observation_shape": [4]
    },
    "selfplay": {
      "num_unroll_steps": 5,
      "td_steps": 10,
      "discount": 0.997
    },
    "dtypes": {
      "model": "float32",
      "action": "int32",
      "target": "float32"
    },
    "logger": {
      "log_dir": "logs",
      "level": "INFO",
      "log_console": false,
      "use_timestamp": true
    },
    "device": "cpu",
    "seed": 42
  }
}
